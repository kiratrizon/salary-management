{
  "version": 2,
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "resources/**/*",
          "routes/**/*",
          "public/**/*",
          "main/**/*",
          "app/**/*",
          "config/**/*",
          "libraries/**/*",
          "models/**/*",
          "vendor/**/*",
          "version.mjs",
          "tmp/**/*"
        ]
      }
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes":[
    {"src":"/(.*)", "dest":"index.mjs"}
  ]
}
